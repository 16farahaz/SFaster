          {{ form_errors(form.ReferenceMag ) }}

          {{form_widget(form.EmplacementMag)}}
          {{form_errors(form.EmplacementMag)}}

          {{ form_widget(form.PrixA) }}

          {{ form_errors(form.PrixA) }}
          </div> 
        
        
          </div>






         
               



            <div class="btn-block">
        <button type="submit" href="{{path('user_index')}}">{{ button_label|default('ADD') }}</button>
        
      </div>
      <div >
      <a class="btn btn-primary btn-icon-split" href="{{ path('user_index') }}" >  <span class="icon text-white-50">
      <i class="fas fa-check"></i>
      </span> <span class="text">Back to list</span></a> 
     
      </div>
      
       
        
        
        
        
     
      {{ form_rest(form) }} 
